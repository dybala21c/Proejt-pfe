<section id="contact-page">
        <div class="container">
            <div class="section-heading text-center">
                <h2><span>Demander Cong√©</span></h2>
              
            </div>
            <div class="row contact-wrap">
                <div class="status alert alert-success" style="display: none"></div>
              
                {{ form_start(form) }}
                    <div class="col-sm-5 col-sm-offset-1">
                        <div class="form-group">
                            <label>Type *</label>
                            <input  class="form-control" required="required" {{ form_widget(form.Type) }}
                        </div>
                        <div class="form-group">
                            <label>Date debut *</label>
                             {{ form_widget(form.DateDebut) }}
                        </div>
                    </div>
                    <div class="col-sm-5">
                        <div class="form-group">
                            <label>Date fin *</label>
                         {{ form_widget(form.DateFin) }}
                        </div>
                       
                        <div class="form-group">
                            <button type="submit" name="submit" class="btn btn-default submit-button">{{ button_label|default('Demander') }} <i class="fa fa-caret-right"></i></button>
                        </div>
                    </div>
                    {{ form_end(form) }}
            
            </div>
        </div>
    </section>
